{
  "name": "libris-book-tracker",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start:server": "node server-dist/index.js",
    "preview": "vite preview",
    "android:init": "vite build && npx cap add android && node scripts/fix-android.js",
    "android:build": "vite build && npx cap sync && npx cap open android",
    "android:clean": "npx cap sync android",
    "fix:android": "node scripts/fix-android.js",
    "fix:cache": "node scripts/nuke-cache.js",
    "generate:assets": "npx capacitor-assets generate --android"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/browser": "^6.0.6",
    "@capacitor/core": "^6.0.0",
    "@capacitor/filesystem": "^6.0.4",
    "@capacitor/local-notifications": "^6.1.3",
    "@capacitor/share": "^6.0.4",
    "@google/genai": "latest",
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^18.0.0",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
